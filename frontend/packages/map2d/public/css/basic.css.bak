    width: 53%;
}
.analysisContent .btn-div {
    float: right;
    height: 30px;
    width: 54%;
}
.analysisContent .btn-div .drawBtn {
    border: 1px solid #e4e4e4;
    border-radius: 3px;
    font-size: 14px;
    height: 36px;
    line-height: 34px;
    margin-right: 5px;
    min-width: 18%;
    padding: 0 24px;
}
.circle-icon{background:url(../images/icon.png) -903px -565px no-repeat;}
.box-icon{background:url(../images/icon.png) -901px -635px no-repeat;}
.polygon-icon{background:url(../images/icon.png) -906px -707px no-repeat;}
.analysisContent .btn-div .isDrawing {
    background: #00aa98;
    border: 1px solid #00aa98;
    color: white;
}
.analysisContent .btn-div .isDrawingGeometry {
    border: 3px solid #00aa98;
    color: white;
}
.aside-content input.small {width:30%;}
.map-select {height:30px; font-size:11px; color:black; padding-left:22px; opacity:0.7; background:url(../images/button.png) -772px -247px no-repeat;}
.map-select:hover {opacity:1;}
.course-result {border-top:2px solid #eeeeee;}
.course-result > p > .time {color:#378ff0; font-size:26px; font-weight:600;}
.course-result > p > .time > .unit {font-size:12px;}
.course-result > p > .distance {display:inline-block; margin-left:10px; color:black; font-size:15px; font-weight:600;}
.course-result > p > .distance > .unit {font-size:12px; }
.course-view li {line-height:32px; text-indent:46px;}
.course-view li.start {width:auto; height:32px; line-height:32px; background:url(../images/button.png) -765px -16px no-repeat;}
.course-view li.via {width:auto; height:32px; line-height:32px; background:url(../images/button.png) -766px -95px no-repeat;}
.course-view li.destination {width:auto; line-height:32px; background:url(../images/button.png) -765px -167px no-repeat;}


/* mapnote */
ul.register {padding-top:20px;}
ul.register li {font-size:14px; width:49%; float:left; margin:0;  padding:0; border-radius:3px; font-family: 's-core-medium'; text-align:center; line-height:40px; background:white; }
ul.register li.on, ul.coordinate-03 li.on {background:#e0faf8;}
ul.register li a {display:block; color:#4e4e4e; cursor:pointer;}

.dialog-content {float:left; height:auto; content:''; display:block; clear:both; line-height:38px; width:88%; margin:0 auto;  padding:20px 0 15px 0; margin-left:6%;}
.dialog-content > ul.half {float:left; width:190px; overflow-y:scroll;   margin-right:10px; margin-bottom:10px; border:1px solid #e1e1e1;}
.dialog-content > ul > li {text-indent:5px; font-size:13px; line-height:32px;}
.dialog-content > ul > li:hover {background:#f5f4f4;}
.dialog-content > ul.full {float:left; overflow-y:scroll; width:100%; border:1px solid #e1e1e1;}
.dialog-content > ul > li {text-indent:5px; font-size:13px; line-height:32px;}
ul.cordinate-search {display:flex; flex-wrap: wrap; flex-direction:row; margin:0; padding:0; justify-content:center;}
ul.cordinate-search li {width:19%; font-size:12px; text-align:center; border-top:1px solid #5a5a5a; border-bottom:1px solid #5a5a5a; border-right:1px solid #5a5a5a; border-radius:3px; float:left; padding:5px 0;}
ul.cordinate-search li:first-child {border:1px solid #5a5a5a;}
ul.cordinate-search li a {display:block;color:white;}
ul.cordinate-search li on a {color:#07ab84;}
ul.cordinate-search li.on {border:1px halfsolid #03d6a3; background:#045848; color:#09dca9;}
ul.cordinate-search li:hover a, ul.cordinate-search li:focus a {color:#09dca9; }
ul.cordinate-search li.on a {color:#09dca9; font-weight:600;}
ul.cordinate-value {display:flex; flex-wrap: wrap; flex-direction:row; justify-content:center;}
ul.cordinate-value li {width:15%; font-size:11px; text-align:center; color:#b1b2b3; font-weight:normal;}
.dialog-distance {position:absolute; top:100px; right:70px; width:290px; height:auto; }
.dialog-distance .dialog-result {float: left;height: auto;display: block;clear: both;line-height: 38px;width: 88%;margin: 0 auto;padding: 0 0 15px 0;margin-left: 6%;}
.dialog-distance .dialog-result-text {font-size: 20px;color: white;line-height: 38px;background: transparent;}
.dialog-content > label {font-size:13px; color:white; line-height:38px; background:transparent;}
select {font-size:13px; background:transparent; background: url("../images/select-dark-button.png") no-repeat right 3px; font-size:13px; border-radius:3px; border:1px solid #585858; -webkit-appearance: none; -moz-appearance: none; appearance: none;}
textarea {width:98%; margin:0 auto; border:1px solid #5a5a5a; background:transparent; font-size:12px; font-family: 's-core-regular';}        
.darkMode select {width:60%; background:black; color:white; text-indent:5px; border:1px solid #5a5a5a; height:32px; border-radius:4px; background: url("../images/select-dark-button.png") no-repeat right 3px; font-size:13px; border-radius:3px; border:1px solid #585858; -webkit-appearance: none; -moz-appearance: none; appearance: none;}
.darkMode select option {color:black;}
.darkMode input[type="text"] {background:black; color:white; font-size:12px; background:transparent; text-indent:5px; border:1px solid #5a5a5a; line-height:38px !important; height:32px; border-radius:4px;}
.dialog-registerPoint {position:absolute; top:100px; right:70px; width:320px; height:auto; }
.dialog-content > ul.pointer {margin:0; padding:0;}
.dialog-content > .pointer-list {border:1px solid #5a5a5a; width:100%; text-indent:3px; font-size:12px; color:white; height:100px;overflow-y:auto}
.dialog-content > .pointer-list ul li {color:rgb(230, 230, 230); font-size:12px; text-indent:5px; line-height:24px;}
.button-delete {float:right; width:24px; height:24px; cursor:pointer; margin-right:10px; background:url(../images/icon.png) -248px -528px no-repeat;}
.dialog-content > ul.pointer li {float:left; width:33.1%; border-radius:3px; line-height:34px; ; margin:0; padding:0; text-align:center; text-indent:22px; border:1px solid #5a5a5a; font-size:13px;}
.dialog-content > ul.pointer li a {color:white; display:block;}
.dialog-content > ul.pointer li.dot {background:url(../images/icon.png) no-repeat -896px -361px;}
.dialog-content > ul.pointer li.line {text-indent:32px; background:url(../images/icon.png) no-repeat -896px -429px;}
.dialog-content > ul.pointer li.polygon {text-indent:28px; background:url(../images/icon.png) no-repeat -900px -494px;}
.dialog-content > ul.pointer li.dot.on {border:1px solid #03d6a3; background:#045848 url(../images/icon.png) no-repeat -687px -361px;}
.dialog-content > ul.pointer li.line.on {border:1px solid #03d6a3; background:#045848 url(../images/icon.png) no-repeat -687px -429px;}
.dialog-content > ul.pointer li.polygon.on {border:1px solid #03d6a3; background:#045848 url(../images/icon.png) no-repeat -691px -494px;}
.dialog-content > ul.pointer li.on a {color:#09dca9; font-weight:600;}
.dialog-content > ul.pointer li.dot:hover a {color:#09dca9; background: url(../images/icon.png) no-repeat -687px -361px;}
.dialog-content > ul.pointer li.line:hover a {color:#09dca9; background: url(../images/icon.png) no-repeat -687px -429px;}
.dialog-content > ul.pointer li.polygon:hover a {color:#09dca9; background: url(../images/icon.png) no-repeat -691px -494px;}
.dialog-symbol {position:absolute; top:300px; left:460px; width:300px; height:auto; }
.symbol-category {width:100% !important;  margin:0 auto !important;}
.symbol-list {display:flex; flex-wrap:wrap; align-content:center; gap:18px;}
.symbol-list li {border-radius:5px; border:1px solid #5a5a5a; text-align:center; width:50px; height:50px; display:inline-block;}
.symbol-image {display:block; width:150px; height:60px; margin:0 auto;}
.symbol-image img {border-radius:5px; border-color:#fff;margin-right: 10px;}
.dialog-infomation {position:absolute; top:300px; left:400px; width:460px; height:auto; }
.dialog-infomation > .dialog-content > label {clear:both; float:left; width:100px; background:rgba(0,0,0,1);font-size:13px; color:white; background:transparent;}
.dialog-infomation > .dialog-content > input[type="text"] {float:left; width:300px !important; line-height:38px;}
.darkMode input[type="text"] {color:#00beb9; background:rgba(42,42,42,.7); border:1px solid #585858;}
.darkMode textarea {color:#00beb9; background:rgba(42,42,42,.7); border:1px solid #585858; border-radius:3px;}
.dialog-content > .title {line-height:40px; font-size:14px; color:white; font-weight:600;}
.dialog-print {position:fixed; left:50%; top:50%; margin-left:-500px; margin-top:-325px; width:1050px; height:650px; }
.dialog-print-content {display:grid; grid-template-columns:660px calc(100% - 660px); padding:25px 5px 25px 25px;}
.dialog-print-content > .print {border:1px solid #484848; height:556px;}
.dialog-print-content > .print-setup {color:white; font-size:13px;}
.dialog-print-content > .print-setup > label {float:left; clear:both; width:100px; line-height:38px; text-align:right; margin-right:20px;}
.dialog-print-content > .print-setup > input {float:left;  line-height:38px; width:auto;}	
.dialog-print-content > .print-setup > select {float:left;  line-height:38px; width:100px}
.dialog-print-content > .print-setup > textarea {float:left; line-height:38px; width:auto; max-width:230px ;max-height:240px}


/* heading */
h4 {font-size:14px; margin:0; padding:09; line-height:30px;}
h4.type02 {float:left; width:190px; font-size:15px; margin:0; padding:09;}
h4.type03 {float:right; width:200px; font-size:15px; margin:0; padding:09;}
textarea {border:1px solid #c5c5c5 !important; }

.attr-operator {border:1px solid #e1e1e1; width:36px; font-size:13px; text-align:center; height:32px;}

/* dialog-search */

.dialog--wrapper {display:block; }
.dialog-search {display:grid; grid-template-columns:1fr 1fr 1fr; position:absolute; left:0; top:40px; padding-top:6px; margin:0; font-family: 's-core-light'; background:white; width:450px; height:360px; box-shadow:0 0 10px rgba(28,28,28,0.5); border-radius:5px;}
.dialog-search ul.sido {overflow-y:scroll; height:300px; border-bottom:1px solid #e1e1e1; border-right:1px solid #e1e1e1; padding-top:10px;}
.dialog-search ul.sido li {font-size:14px; line-height:28px; text-indent:20px;}
.dialog-search ul.gun-gu {overflow-y:auto; height:300px; border-bottom:1px solid #e1e1e1; border-right:1px solid #e1e1e1; padding-top:10px;}
.dialog-search ul.gun-gu li {font-size:14px; line-height:28px; text-indent:20px;}
.dialog-search ul.dong {overflow-y:auto; height:300px; border-bottom:1px solid #e1e1e1; border-right:1px solid #e1e1e1; padding-top:10px;}
.dialog-search ul.dong li {font-size:14px; line-height:28px; text-indent:20px;}
.dialog-search ul.sido li:hover, .dialog-search ul.gun-gu li:hover, .dialog-search ul.dong li:hover {cursor:pointer; background:#e9f5fb;}
.dialog-search ul.sido li.on, .dialog-search ul.gun-gu li.on, .dialog-search ul.dong li.on {cursor:pointer; background:#e9f5fb;}
.dialog-btn {grid-column: span 3; text-align:right; height:50px; margin-top:10px;}

/* dialog-guide */
.dialog-guide {position:fixed; left:50%; top:50%; margin-left:-250px; margin-top:-200px; padding:25px; width:500px; height:100px; border-radius:10px; background:rgba(0,0,0,0.8);}
.dialog-guide > .bullet {float:left; width:50px; height:70px; background:url(../images/button.png) -763px -345px no-repeat;}
.dialog-guide > .txt {display:inline-block; font-size:14px; color:white;}

.graph-wrapper {

}

/* pagination */

.paging {
    width: 100%;
    position: relative;
    height: 30px;
    float: left;
    margin-top: 10px;
    text-align: center;
    padding: 0;
    display: flex;
    justify-content: center;
    margin-bottom: 5px;
    font-size:12px;
  }
  .paging button {
    margin: 0;
    padding: 0;
    width: 24px;
    height: 24px;
    background: white;
    border: 1px solid rgba(216, 216, 216, 0.8);
    display: inline-block;
    line-height: 24px;
    text-align: center;
    vertical-align: middle;
    margin-left: 5px;
    border-radius: 30px;
    color: #777777;
  }
  .paging button.current {
    background: black;
    border: 1px solid black;
    color: white;
    font-weight: 600;
  }
  .paging button.start, .paging button.pre, .paging button.next, .paging button.end {
    width: 26px;
    height: 26px;
    border: 0;
    opacity: 0.6;
  }
  .paging button.start {
    background: url(../images/paging-icon.png) 6px 7px no-repeat;
  }
  .paging button.pre {
    margin-right: 15px;
    background: url(../images/paging-icon.png) -69px 7px no-repeat;
  }
  .paging button.next {
    margin-left: 15px;
    background: url(../images/paging-icon.png) -138px 7px no-repeat;
  }
  .paging button.end {
    background: url(../images/paging-icon.png) -196px 7px no-repeat;
  }
  .paging button:hover, .paging button:focus, .paging button:active {
    border: 1px solid silver;
  }
  .paging button.start:hover, .paging button.pre:hover, .paging button.next:hover, .paging button.end:hover {
    border: 0;
    opacity: 1;
  }

/* 스피너 스타일 */
.modal {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.68);
    z-index: 999999;
    justify-content: center;
    align-items: center;
    display: flex;
}
.modal .modal-content {
    padding: 20px;
    position: relative;
  }
.spinner {
    border: 7px solid rgba(201, 231, 255, 0.2);
    border-top: 7px solid rgb(44, 226, 239);
    border-radius: 50%;
    width: 65px;
    height: 65px;
    animation: spin 0.9s linear infinite;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
  }


  /* 출력 */

label.print-option-label {
  color: white;
  font-size: 12px;
  margin-right: 5px;
}
@keyframes spin {
    0% {
      transform: translate(-50%, -50%) rotate(0deg);
    }
    100% {
      transform: translate(-50%, -50%) rotate(360deg);
    }
  }
  
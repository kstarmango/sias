body { margin:0; padding:0; height:100vh; overflow-wrap:break-word; font-family:"s-core-regular", Arial, sans-serif; line-height: 1.5em;}
:lang(ko) {word-break:keep-all;}
img {max-width:100%; height:auto; border:0; vertical-align: middle;}
[class] {margin:0; padding:0; box-sizing:border-box;	list-style:none;	border:0;	background-color:transparent;	border-collapse:collapse;	border-spacing:0;	-webkit-appearance:none;	appearance:none;	}
[class]::after {box-sizing:border-box;}
[class]:where(ol ul) {list-style:none;}
[class]:where(button, fieldset, iframe, select, textarea) {border:0;}
[class]:where(button, dialog, input, mark, meter, progress) {background-color:transparent;}
[class]:where(table) {border:0; border-collapse:collapse;	border-spacing:0;}
[class]:where(button, input, meter, progress select, textarea) {-webkit-appearance:none; appearance:none;}
ol, ul {list-style: none;}
ul, li {margin:0; padding:0; border:0;}
table, caption, tbody, tfoot, thead, tr, th, td, input, select, button, header, h1, nav, ul {margin: 0; padding: 0; border: 0; font-size: 100%; font-family:"s-core-regular", Arial, sans-serif;  }
table,  td {border-collapse: collapse; border-spacing: 0;}
tr {display:table-row; vertical-align:inherit; border-color:inherit;}
td {display:table-cell;}
body, div, p, h1,h2, h3, h4, h5, h6, ul, ol, li, dl, dt, dd, table, th, td, form, fieldset, legend,
input, textarea, blockquote, button, strong, em, address {-webkit-text-size-adjust:none; font-weight:normal;color:#191919;}
h1, h2 ,h3, h4, h5, h6 {font-size: 100%; font-weight: normal; font-family:"s-core-medium", Arial, sans-serif;}
img, fieldset { padding: 0; border: none; }
legend, caption { width:0; height:0; font-size:0; line-height:0; overflow:hidden; visibility:hidden; display: none;}
a {text-decoration: none; text-decoration:none; color:#000;}
input[type="date"] {-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none}
/* input[type="date"]::-webkit-clear-button,input[type="date"]::-webkit-inner-spin-button {display: none;}
input[type="date"]::-webkit-calendar-picker-indicator {background: transparent; z-index: 1;} */
input[type="checkbox"] { margin-bottom:5px; margin-right:2px;}
input[type=number], input[type="text"] {border:1px solid #e4e4e4; height:32px; text-indent:10px;}.
select::-ms-expand {display: none;}
button {cursor:pointer;}
.blind {display:none;}
input[type="text"]:focus {outline: 0;}

/* 레이아웃 */
#container {width:100%; height:100%; position:relative; margin:0; padding:0; display:grid; grid-template-columns:70px calc(100% - 70px); overflow-y:hidden;}
nav {background:#263840; height:100%; position:relative;}
main {background:silver; height:100%; width:100%; position:relative;}
#map {width:100%; height:100%; background:url(../images/map.png) no-repeat; background-size:cover; overflow-y:hidden;}
nav ul {margin:0; padding:0;}
nav ul li {font-size:12px; height:73px; line-height:114px; color:white; text-align:center;}
nav ul li.logo {line-height:18px; padding-top:20px; font-size:14px; font-family: 's-core-medium';  border-bottom:1px solid rgba(255,255,255,0.1);}
nav ul li.layer {width:70px; height:73px; background:url(../images/icon.png) 22px 3px; border-bottom:1px solid rgba(255,255,255,0.1);}
nav ul li.timeSeries {width:70px; height:73px; background:url(../images/icon.png) 23px -102px; border-bottom:1px solid rgba(255,255,255,0.1);}
nav ul li.search {width:70px; height:73px; background:url(../images/icon.png) 22px -199px; border-bottom:1px solid rgba(255,255,255,0.1);}
nav ul li.anaylisys {width:70px; height:73px; background:url(../images/icon.png) 21px -300px; border-bottom:1px solid rgba(255,255,255,0.1);}
nav ul li.coordinate {width:70px; height:73px; background:url(../images/icon.png) 21px -410px; border-bottom:1px solid rgba(255,255,255,0.1);}
nav ul li.mapnote {width:70px; height:73px; background:url(../images/icon.png) 23px -524px; border-bottom:1px solid rgba(255,255,255,0.1);}
nav ul li.layer.on {width:70px; height:73px; background:#00beb9 url(../images/icon.png) 22px 3px;}
nav ul li.timeSeries.on {width:70px; height:73px; background:#00beb9 url(../images/icon.png) 23px -102px;}
nav ul li.search.on {width:70px; height:73px; background:#00beb9 url(../images/icon.png) 22px -199px;}
nav ul li.anaylisys.on {width:70px; height:73px; background:#00beb9 url(../images/icon.png) 21px -300px;}
nav ul li.coordinate.on {width:70px; height:73px; background:#00beb9 url(../images/icon.png) 21px -410px;}
nav ul li.mapnote.on {width:70px; height:73px; background:#00beb9 url(../images/icon.png) 23px -524px;}
nav ul li.layer:hover {transition-duration:.5s; width:70px; height:73px; background:#00beb9 url(../images/icon.png) 22px 3px;}
nav ul li.timeSeries:hover {transition-duration:.5s; width:70px; height:73px; background:#00beb9 url(../images/icon.png) 23px -102px;}
nav ul li.search:hover {transition-duration:.5s; width:70px; height:73px; background:#00beb9 url(../images/icon.png) 22px -199px;}
nav ul li.anaylisys:hover {transition-duration:.5s; width:70px; height:73px; background:#00beb9 url(../images/icon.png) 21px -300px;}
nav ul li.coordinate:hover {transition-duration:.5s; width:70px; height:73px; background:#00beb9 url(../images/icon.png) 21px -410px;}
nav ul li.mapnote:hover {transition-duration:.5s; width:70px; height:73px; background:#00beb9 url(../images/icon.png) 23px -524px;}
nav ul li a {color:white; display:inline-block; }
nav ul li.on a {font-weight:600;}
nav ul li {cursor:pointer;}
nav .user-info {display:inline-block; position:absolute; margin:0 auto; text-align:center;border-top:1px solid rgba(255,255,255,0.1); font-size:11px; line-height:105px; color:white; bottom:70px; width:70px; height:70px; background:url(../images/user.png) 23px 14px no-repeat;}
nav .logout {position:absolute; bottom:40px; text-indent:0; font-size:12px; text-align:center; border:1px solid rgba(255,255,255,0.2); margin-left:4px; border-radius:6px; width:60px; padding:6px 0; color:white;}
nav .logout:hover {opacity:1;}

/* toolbox */
#toolbox {margin:0; padding:0; position:absolute; top:20px; right:20px;}
#toolbox button {display:block; width:40px; height:40px; margin:0; padding:0; border-right:1px solid rgba(100,100,100,0.8); border-left:1px solid rgba(100,100,100,0.8); border-bottom:1px solid rgba(100,100,100,0.8); background:rgba(0,0,0,0.8);}
#toolbox button:first-child {border-top-left-radius:4px; border-top-right-radius:4px; border-top:1px solid rgba(100,100,100,0.8);}
#toolbox button:last-child {border-bottom-left-radius:4px; border-bottom-right-radius:4px;}
#toolbox button.compass {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -171px 7px;}
#toolbox button.home {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -170px -54px;}
#toolbox button.length {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -169px -110px;}
#toolbox button.area {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -172px -172px;}
#toolbox button.print {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -171px -230px;}
#toolbox button.save {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -171px -301px;}
#toolbox button.compass:hover/* , #toolbox button.compass:focus */ {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -245px 7px;}
#toolbox button.home:hover/* , #toolbox button.home:focus */ {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -244px -54px;}
#toolbox button.length:hover, #toolbox button.length.active/* , #toolbox button.length:focus */ {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -243px -110px;}
#toolbox button.area:hover, #toolbox button.area.active/* , #toolbox button.area:focus */ {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -246px -172px;}
#toolbox button.print:hover/* , #toolbox button.print:focus */ {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -245px -230px;}
#toolbox button.save:hover/* , #toolbox button.save:focus */ {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -245px -301px;}
#toolbox-view {margin:0; padding:0; position:absolute; top:300px; right:20px;}
#toolbox-view button {display:block; width:40px; height:40px; background:rgba(0,0,0,0.8);}
#toolbox-view button {display:block; width:40px; height:40px; margin:0; padding:0; border-right:1px solid rgba(100,100,100,0.8); border-left:1px solid rgba(100,100,100,0.8); border-bottom:1px solid rgba(100,100,100,0.8); background:rgba(0,0,0,0.8);}
#toolbox-view button.expand {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -172px -368px;}
#toolbox-view button.reduce {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -171px -436px;}
#toolbox-view button.expand:hover, #toolbox-view button.expand:focus {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -245px -368px;}
#toolbox-view button.reduce:hover, #toolbox-view button.reduce:focus {background:rgba(0,0,0,0.8) url(../images/icon.png) no-repeat -244px -436px;}
#toolbox-view button:first-child {border-top-left-radius:4px; border-top-right-radius:4px; border-top:1px solid rgba(100,100,100,0.8);}
#toolbox-view button:last-child {border-bottom-left-radius:4px; border-bottom-right-radius:4px;}
.toolbox-txt {display:none; width:auto; float:right; position:relative; z-index:999999; margin-right:45px; margin-top:3px;}
.toolbox-txt > .rect { float:right; margin-top:-17px; margin-right:-8px; width: 0; height: 0; border-top: 5px solid transparent; border-right: 5px solid transparent; border-bottom: 5px solid transparent; border-left: 5px solid #292929;}
.toolbox-txt > .title { float:left; clear:both; font-size:11px; background:#292929; color:white; border-radius:5px; line-height:24px;  padding:0 10px;}
#toolbox button.compass:hover .toolbox-txt {display:block;}
#toolbox button.home:hover .toolbox-txt {display:block;}
#toolbox button.length:hover .toolbox-txt {display:block;}
#toolbox button.area:hover .toolbox-txt {display:block;}
#toolbox button.print:hover .toolbox-txt {display:block;}
#toolbox button.save:hover .toolbox-txt {display:block;}

/* aside */
aside {width:380px; height:100%; position:absolute;}
aside.basic {background:white; height:100%; position:absolute; }
.basic-scroll {position: relative; margin:0; padding:0; overflow: scroll; height:calc(100% - 140px); }
aside.gray-bg {background:#f0f8f9; margin:0; padding:0;}
.aside-search {margin:0 auto;}
.itemSelect {height:38px; color:white; font-size:14px; font-weight:600; border:1px solid #00c2ae; background:#00c2ae; line-height:38px; text-indent:10px;}
.itemSelect-content {background:#f3f3f3; line-height:26px; display:grid; grid-template-columns:repeat(2, 1fr);}
.itemSelect-content > label {font-size:13px; color:#484747; padding-left:10px;}
.itemUnselect {display:block; text-align:left; color:#4e4e4e; cursor:pointer; font-family: 's-core-light';  font-weight:600; font-size:14px; line-height:33px; border:1px solid #eeeeee; background:#eeeeee url(../images/button.png) 210px -39px no-repeat; text-indent:10px;}
.itemUnselect-select {display:block; color:white; width:100%; cursor:pointer; position:relative; height:calc(100% - 480px); line-height:38px; font-family: 's-core-light'; font-weight:600; font-size:14px; padding-bottom:0; border:1px solid #eeeeee; background:#0f3b90 url(../images/button.png) 200px -278px no-repeat; text-indent:10px;}
.itemUnselect:hover {background:#e4e4e4 url(../images/button.png) 210px -39px no-repeat; border:1px solid #eeeeee;}
.itemUnselect .number {display:inline-block; font-size:12px; text-indent:5px; color:#0c4fd6; font-weight:600;}
.itemUnselect .number::before {content: '('; color:gray;}
.itemUnselect .number::after {content: '건)'; color:gray;}
.itemUnselect-select .number {display:inline-block; color:white; font-size:12px; text-indent:5px; font-weight:600;}
.itemUnselect-select .number::before {content: '('; color:rgb(216, 216, 216);}
.itemUnselect-select .number::after {content: '건)'; color:rgb(216, 216, 216);}
.button-location {display:inline-block; cursor:pointer; margin-top:20px; margin-left:380px; position:absolute; padding-right:30px; height:40px; background:rgba(0, 0, 0, 0.8) url(../images/bullet-location.png) 8px 10px no-repeat;}
.button-location:hover {background:rgba(0, 0, 0, 0.8) url(../images/bullet-location.png) -39px 10px no-repeat;}
.button-location.active {background:rgba(0, 0, 0, 0.8) url(../images/bullet-location.png) -39px 10px no-repeat;}
p.address {display:block; position:absolute; top:20px; left:380px; border-top-right-radius:5px; border-bottom-right-radius:5px; padding-right:30px; color:white; font-size:14px; width:auto !important; background:rgba(0, 0, 0, 0.8); line-height:40px; margin-left:30px;}
/* .button-location:hover .dialog--wrapper {display: block;} */
.layerSelect {height:42px; color:white; font-size:14px; font-weight:600; border:1px solid #00c2ae; background:#00c2ae url(../images/icon.png) -912px -782px no-repeat; line-height:38px; text-indent:40px;}
.layerUnselect {display:block; height:42px; color:#4e4e4e; font-family: 's-core-light'; font-weight:600; font-size:14px; line-height:38px; border:1px solid #e2e2e2; background:#e3e3e3 url(../images/icon.png) -912px -834px no-repeat;; text-indent:40px;}
.layerList {height:37px; font-size:14px; line-height:37px; background:white; border-bottom:1px solid #dfdfdf;}
.layerList-middle {width:23px; height:35px; background: url(../images/button.png) -340px -76px no-repeat; margin:0; padding:0;}
.layerList-last {width:23px; height:35px; background: url(../images/button.png) -340px -13px no-repeat; margin:0; padding:0;}
.layerList-on {width:150px; height:35px; text-indent:26px; background:url(../images/icon.png) -641px -844px no-repeat;}
.layerList-off {width:150px; height:35px; text-indent:26px; background:url(../images/icon.png) -490px -844px no-repeat;}
.yScroll {overflow-y:scroll;}
.aside-content {width:100%; margin:0; padding:0; background:white; position:relative; width:100%; height:calc(100% - 80px);overflow-y:auto;}
.itemUnselect-box {height:100%; background:white; overflow-x:hidden; width:100%; background:white; margin:0 auto; overflow-y:scroll; background:white; border-left:1px solid #d7d7d7; border-bottom:1px solid #d7d7d7; border-right:1px solid #d7d7d7; padding:0 0 40px 0; font-size:14px; }
.item-box {width:100%; margin:0; display:flex; align-items: center; padding:0; height:40px; float:left; line-height:20px; border-bottom:1px solid #e2e2e2 ;}
.item-box:hover, .item-box:focus, .item-box.active {background:#eff6fa; cursor:pointer;}
.item-box .address {width:300px; margin-right:20px; font-size:13px; white-space:nowrap; text-overflow: ellipsis; overflow: hidden; font-family: 's-core-regular'; padding-left:10px; float:left; text-align:left; text-indent:0;}
.item-box button.go-map {float:right; width:20px; height:20px; margin-right:10px; opacity:0.6; background:url(../images/icon-link.png) no-repeat;}
.item-box button.go-map:hover {opacity:1;}
.button-area {position:absolute; width:87%; bottom:15px; margin-left:24px; background:white; height:40px;}
.button-more {font-size:13px; float:left; width:49%; border-radius:5px; border:1px solid #d7d7d7; color:black; padding:8px 0;}
.button-more:hover {border:1px solid #777777;}
.button-reset {font-size:13px; width:49%; float:right; border-radius:5px; border:1px solid #d7d7d7; right:7.5%; color:black; padding:8px 0; text-indent:20px; background:url(../images/button.png) -275px -380px no-repeat;}
.button-reset:hover {border:1px solid #777777;}

/* analysis */
ul.tabMenu {padding-top:10px; border-bottom:1px solid #eeeeee; margin-top:-1px;}
ul.tabMenu li {width:25%; line-height:47px; float:left; text-align:center; font-size:15px; font-weight:600; font-family: 's-core-light'; color:black;}
ul.tabMenu li.on {border-bottom:4px solid #00beb9 !important; color:#00beb9; }
ul.tabMenu li.on a {color:#00beb9;}
ul.tabMenu li:hover a {color:#00beb9; }
ul.tabMenu li a {display:block;}
ul.tabMenu.type03 li {width:32.3%; line-height:47px; display:inline-block;  text-align:center; font-size:15px;}
.analysisSelect {line-height:38px; background:#cfefec url(../images/button.png) 200px -105px no-repeat; text-indent:13px; color:#014e48; font-family: 's-core-regular'; font-weight:600; font-size:14px; border-left:4px solid #00beb9; border-radius:4px;}
.analysisunSelect {line-height:38px; background:#f6f6f6 url(../images/button.png) 200px -38px no-repeat; text-indent:13px; font-family: 's-core-light'; font-weight:600; font-size:14px; border-left:4px solid #e3e3e3; border-radius:4px;}
.analysisContent {font-size:13px; background:white; padding:15px 0 15px 10px;}
.analysisContent > label {font-size:13px; color:black; padding-left:10px; font-family: 's-core-extralight'; font-weight:600; line-height:40px; width:120px; text-align:right; margin-right:10px; display:inline-block;}
.analysisContent > input {width:53%;}
.analysisContent > label.width100 {font-size:13px; color:black; padding-left:10px; font-family: 's-core-extralight'; font-weight:600; line-height:40px; width:100px; text-align:right; margin-right:10px; display:inline-block;}
.analysisContent > input.width48 {width:44%;}
.analysisContent>select {
    -webkit-appearance: none;
    appearance: none;
    background: url(/static/media/select-button.04ed61b….png) no-repeat right 5px;
    border: 1px solid #e4e4e4;
    border-radius: 0;
    color: #464646;
    font-size: 13px;
    height: 34px;
    margin: 0;
    padding: 0 14px 0 0;
